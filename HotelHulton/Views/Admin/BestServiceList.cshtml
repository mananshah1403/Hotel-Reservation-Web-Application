@model IEnumerable<HotelComponent.BestService>

@{
    ViewBag.Title = "BestServiceList";
}
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<h2>BestServiceList</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HotelID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HotelID)
        </td>
        
    </tr>
}

</table>
